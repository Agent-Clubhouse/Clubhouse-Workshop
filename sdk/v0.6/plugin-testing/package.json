{
  "name": "@clubhouse/plugin-testing",
  "version": "0.6.0",
  "description": "Test utilities for Clubhouse plugins",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clubhouse/plugin-types": "file:../plugin-types"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "@testing-library/react": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "peerDependenciesMeta": {
    "@testing-library/react": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "tsup": "^8.0.0",
    "@types/react": "^19.0.0"
  },
  "keywords": [
    "clubhouse",
    "plugin",
    "testing",
    "mock"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Agent-Clubhouse/Clubhouse-Workshop",
    "directory": "sdk/v0.6/plugin-testing"
  }
}
